@using System.Collections.Generic;
@{ 
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
    var list=ViewBag.list as Dictionary<string,int>;
}

    <div class="row bg-primary">
        <div class="col-lg-12">
            <h1 class="page-header" style="text-align:left;">Dashboard</h1>
        </div>
    </div>

    <div class="row col-md-12" style="margin-top:50px;">
        @foreach (var item in list)
        { 
        <div class="col-md-2" style="border-radius:20px; background-color:aquamarine; height:200px; margin:42px;">
            <div >
                <div>
                    <h3>@item.Key</h3>
                    <h4>@item.Value</h4>
                </div>
            </div>
        </div>
        }
    </div>
